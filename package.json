{
  "name": "@mayntri/sdk",
  "version": "1.0.0",
  "type": "module",
  "homepage": "http://mayntri.github.io/sdk",
  "scripts": {
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{ts}'",
    "lint:fix": "eslint --fix 'src/**/*.{ts}'",
    "format": "prettier --write src//**/*.{ts} --config ./.prettierrc",
    "dev": "start-storybook -p 6006"
  },
  "dependencies": {
    "axios": "1.2.3 || >=1.*"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "babel-loader": "^9.1.2",
    "eslint": "^8.32.0",
    "eslint-config-standard-with-typescript": "^31.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-dts": "^1.7.1",
    "vite-plugin-linter": "^2.0.2",
    "vite-tsconfig-paths": "^4.0.3"
  },
  "peerDependencies": {
    "axios": "1.2.3 || >=1.*"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/sdk.es.js",
      "require": "./dist/sdk.umd.js"
    }
  },
  "main": "./dist/sdk.umd.js",
  "module": "./dist/sdk.es.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "@mayntri:sdk": "https://npm.pkg.github.com"
  }
}
